<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duakhan.AsanZindagi">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".tutor_Profile" />
        <activity android:name=".Doctor_Profile" />
        <activity android:name=".Profile" />
        <activity android:name=".share" />
        <activity android:name=".setting_nav" />
        <activity android:name=".FavActivity" />
        <activity android:name=".logout" />
        <activity android:name=".apointments_NavActivity" />
        <activity android:name=".home_Activity" />
        <activity android:name=".Pasword_Activity" />
        <activity android:name=".signup" />
        <activity android:name=".login" />
        <activity android:name=".splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>